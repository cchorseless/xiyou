{
    "include": ["src", "../../shared"],
    "compilerOptions": {
        "target": "es2017",
        "lib": ["es2017"],
        "types": ["@moddota/panorama-types"],
        "moduleResolution": "node",
        "strict": true,
        "strictPropertyInitialization":false,
        "preserveSymlinks": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "jsx": "react"
    }
}
