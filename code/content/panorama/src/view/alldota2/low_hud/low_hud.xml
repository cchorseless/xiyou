<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/hudstyles.vcss_c" />
		<include src="s2r://panorama/styles/hud/hud_reborn.vcss_c" />
		<include src="s2r://panorama/styles/custom_ui.vcss_c" />
		<include src="s2r://panorama/styles/hud/dota_hud_xp.vcss_c" />
		<include src="s2r://panorama/styles/hud/dota_hud_inventory.vcss_c" />
		<include src="s2r://panorama/styles/hud/dota_hud_aghs_status_display.vcss_c" />
		<include src="./low_hud.less" />
	</styles>
	<scripts>
		<include src="s2r://panorama/scripts/sequence_actions.vjs_c" />
		<!-- <include src="file://{resources}/scripts/custom_game/common.js" /> -->
		<include src="./low_hud.tsx" />
	</scripts>

	<Panel class="CustomHudRoot" hittest="false">
		<!-- <Panel id="DragTest"/> -->
		<Panel id="ConsumablesContainer"/>
		<Panel id="TeamBossContainer"/>
		<Panel id="FreezePanelContainer"/>
		<Panel id="RightTopButtons"/>
		<Panel id="RewardsToastContianer" hittest="false"/>

		<Panel id="HUDContents" class="CustomHudContents" hittest="false">
			<DOTAMinimap id="OverlayMap" renderbackground="true" />

			<Panel id="DeleteAbilityDragEndArea">
				<Label text="#DeleteAbilityDragEndAreaLabel" hittest="false" />
			</Panel>

			<Panel id="MainTask" />

			<Panel id="DungeonVote" hittest="false"/>

			<Panel id="lower_hud" hittest="false" disallowedstyleflags="hover,descendantfocus">
				<Panel id="BuffContainer" hittest="false">
					<DOTABuffList id="buffs" showdebuffs="false" />
					<DOTABuffList id="debuffs" showbuffs="false" />
				</Panel>

				<Panel id="center_block" hittest="false" disallowedstyleflags="hover,descendantfocus">
					<Panel id="left_flare" />
					<Panel id="center_bg" />
					<!-- <Panel id="HUDSkinPortrait" hittest="false"/>
					<Panel id="HUDSkinXPBackground" hittest="false"/> -->
					<Panel id="PortraitGroup">
						<DOTAParticleScenePanel id="PortraitStreakParticle" particleonly="true" cameraOrigin="-300 0 150" lookAt="0 0 180" fov="50" hittest="false"/>
						<Panel id="PortraitStreakParticleBorder" hittest="false" class="" />
						<Panel id="PortraitBacker" hittest="false"/>
						<Panel id="PortraitBackerColor" hittest="false"/>
						<Panel id="PortraitContainer" hittest="false">
							<DOTAPortrait id="portraitHUD" class="PortraitLocation" onmouseover="DOTAHUDShowVisionRange();" onmouseout="DOTAHUDHideVisionRange();"/>
							<DOTAPortrait id="portraitHUDOverlay" class="PortraitLocation"/>
							<Image id="RightSideHeroBlur" src="panel://portraitHUD" hittest="false"/>
							<Panel id="SilenceIcon" hittest="false" always-cache-composition-layer="true" />
							<Panel id="MutedIcon" hittest="false" always-cache-composition-layer="true" />
							<Panel id="DeathGradient"/>
						</Panel>
					</Panel>
					<DOTAMultiUnit id="multiunit" class="PortraitLocation" />
					<DOTAHUDDeathPanel id="death_panel_buyback" />
					<Panel id="custom_xp"/>
					<!-- <DOTAXP id="xp" hittest="false" always-cache-composition-layer="true" require-composition-layer="true" /> -->
					<Panel id="stats_container" hittest="false">
						<Panel id="stats_container_bg" hittest="false" />
						<Panel id="HUDSkinStatBranchBG" hittest="false"/>
						<!-- <Panel id="HUDSkinStatBranchGlow" hittest="false"/> -->
						<Panel id="StatsContainer" hittest="false">
							<Panel id="Stats" require-composition-layer="true" always-cache-composition-layer="true" hittest="false" hittestchildren="false">
								<Panel id="Damage" class="StatIconLabel">
									<Label id="DamageLabel" class="MonoNumbersFont StatRegionLabel CombinedLabel" text="{s:combined_damage}" />
									<Label id="DamageBaseLabel" class="MonoNumbersFont StatRegionLabel BaseLabel" text="{s:damage}" />
									<Label id="DamageModifierLabel" class="MonoNumbersFont StatRegionLabel StatModifier" text="{s:bonus_damage}" />
									<Panel id="DamageIcon" class="StatIcon"/>
								</Panel>
								<Panel id="Armor" class="StatIconLabel">
									<Label id="DamageResist" class="MonoNumbersFont StatRegionLabel" text="{d:armor_resistance}%" />
									<Label id="ArmorLabel" class="MonoNumbersFont StatRegionLabel CombinedLabel" text="{s:combined_armor}" />
									<Label id="ArmorBaseLabel" class="MonoNumbersFont StatRegionLabel BaseLabel" text="{s:armor}" />
									<Label id="ArmorModifierLabel" class="MonoNumbersFont StatRegionLabel StatModifier" text="{s:bonus_armor}" />
									<Panel id="ArmorIcon" class="StatIcon" />
								</Panel>
								<Panel id="MoveSpeed" class="StatIconLabel">
									<Label id="MoveSpeedLabel" class="MonoNumbersFont StatRegionLabel CombinedLabel" text="{s:combined_move_speed}" />
									<Label id="MoveSpeedBaseLabel" class="MonoNumbersFont StatRegionLabel BaseLabel" text="{d:base_move_speed}" />
									<Label id="MoveSpeedModifierLabel" class="MonoNumbersFont StatRegionLabel StatModifier" text="{s:bonus_move_speed}" />
									<Panel id="MoveSpeedIcon" class="StatIcon" />
								</Panel>
							</Panel>
							<Panel id="Attribute" always-cache-composition-layer="true" require-composition-layer="true" hittest="false">
								<Panel id="Strength" class="AttrIconContainer">
									<Panel class="HighlightStatIcon" hittest="false" />
									<Label id="StrengthLabel" class="MonoNumbersFont StatRegionLabel CombinedLabel" text="{s:combined_strength}" />
									<Label id="StrengthBaseLabel" class="MonoNumbersFont StatRegionLabel BaseLabel" text="{s:strength}" />
									<Label id="StrengthModifierLabel" class="MonoNumbersFont StatRegionLabel StatModifier" text="{s:strength_bonus}" />
									<Panel id="StrengthIcon" class="StatIcon" hittest="false" />
								</Panel>
								<Panel id="Agility" class="AttrIconContainer">
									<Panel class="HighlightStatIcon" hittest="false" />
									<Label id="AgilityLabel" class="MonoNumbersFont StatRegionLabel CombinedLabel" text="{s:combined_agility}" />
									<Label id="AgilityBaseLabel" class="MonoNumbersFont StatRegionLabel BaseLabel" text="{s:agility}" />
									<Label id="AgilityModifierLabel" class="MonoNumbersFont StatRegionLabel StatModifier" text="{s:agility_bonus}" />
									<Panel id="AgilityIcon" class="StatIcon" hittest="false" />
								</Panel>
								<Panel id="Intellect" class="AttrIconContainer">
									<Panel class="HighlightStatIcon" hittest="false" />
									<Label id="IntellectLabel" class="MonoNumbersFont StatRegionLabel CombinedLabel" text="{s:combined_intellect}" />
									<Label id="IntellectBaseLabel" class="MonoNumbersFont StatRegionLabel BaseLabel" text="{s:intellect}" hittest="false" />
									<Label id="IntellectModifierLabel" class="MonoNumbersFont StatRegionLabel StatModifier" text="{s:intellect_bonus}" />
									<Panel id="IntellectIcon" class="StatIcon" hittest="false" />
								</Panel>
							</Panel>
						</Panel>
						<Panel id="StatsTooltipRegion" />
					</Panel>
					<Panel id="UnitNameContainer">
						<DOTAUnitName id="unitname" hittest="false" />
					</Panel>
					<DOTAHUDLevelStatsFrame id="level_stats_frame" hittest="false" />
					<DOTALevelUpButton id="levelup" hittest="false" />
					<DOTAHealthMana id="health_mana">
						<Panel id="HealthManaContainer" hittest="false">
							<Panel id="HealthContainer" hittest="false">
								<Label id="CustomHealthLabel" class="MonoNumbersFont" hittest="false" style="text-align: center;font-weight: bold;font-size: 18px;color: white;width: 332px;margin-top: 1px;margin-bottom: 2px;horizontal-align: center;vertical-align: center;text-shadow: 1px 1px 0px 2.0 #000000;overflow: noclip;z-index: 4;" />
								<Label id="CustomHealthRegenLabel" class="MonoNumbersFont" hittest="false" style="color: #3ed038;font-size: 14px;text-shadow: 2px 2px 0px 1.0 #00000066;font-weight: bold;margin-top: -1px;margin-right: 4px;text-align: right;vertical-align: center;padding-right: 2px;horizontal-align: right;z-index: 4;" />
							</Panel>
						</Panel>
					</DOTAHealthMana>
					<!-- <Panel id="HUDSkinAbilityContainerBG" hittest="false" /> -->
					<!-- <Panel class="AbilityInsetShadowLeft"/> -->
					<Panel id="SecondaryAbilityContainer" hittest="false">
						<!-- <DOTASecondaryAbilitiesBar id="SecondaryAbilitiesBar" class="HideWhenCanLevelAbilities" /> -->
						<Panel class="TertiaryAbilityContainer" hittest="false">
							<Panel id="TertiaryAbilitiesBar" class="ShowSecondaryAbilitiesBar">
								<Panel id="Contents" hittest="false">
									<Panel id="AbilityButtons" hittest="false">
									</Panel>
								</Panel>
							</Panel>
						</Panel>
					</Panel>
					<Panel id="AbilitiesAndStatBranch" hittest="false">
						<Panel class="LeftRightFlow" hittest="false">
							<Panel id="NPCDialogEvents" />
							<!-- <DOTATalentDisplay id="StatBranch"/> -->
							<Panel id="abilities" class="abilities" hittest="false" />
							<!-- <DOTAAghsStatusDisplay id="AghsStatusContainer" /> -->
						</Panel>
						<!-- <ProgressBar id="MorphProgress" min="0" max="1" value="0.5" /> -->
					</Panel>
					<!-- <Panel class="AbilityInsetShadowRight" require-composition-layer="true" always-cache-composition-layer="true"/> -->
					<DOTAUnitEconItem id="econ_item"/>
					<Panel id="right_flare"/>
					<Panel id="inventory" class="inventory">
						<Panel id="inventory_items" hittest="false" require-composition-layer="true" always-cache-composition-layer="true">
							<Panel id="InventoryContainer">
								<!-- <Panel id="InventoryBG" class="InventoryBackground" hittest="true" /> -->
								<!-- <Panel id="HUDSkinInventoryBG" class="InventoryBackground" hittest="false"/> -->
								<Panel id="inventory_list_container" hittest="false" />
								<Panel id="inventory_backpack_list" class="inventory_list" hittest="false" />
								<Panel id="BackpackShadow" hittest="false" />
							</Panel>
						</Panel>
					</Panel>

					<Panel id="left_composition_layer_container" require-composition-layer="true">
						<Image id="left_inset_center"/>
						<Panel id="left_attribute_container"/>
					</Panel>

					<Panel id="inventory_composition_layer_container" require-composition-layer="true">
						<Image id="inventory_inset_center"/>
						<Panel id="inventory_tpscroll_container"/>
						<Panel id="inventory_neutral_slot_container"/>
					</Panel>
				</Panel>

				<Panel id="right_block" hittest="false" disallowedstyleflags="hover,descendantfocus">
					<!-- 战斗力 -->
					<Panel id="HeroPowerContainer" onmouseover="DOTAShowTextTooltip('#Tooltip_HeroPower')" onmouseout="DOTAHideTextTooltip()">
						<Image id="HeroPowerIcon" hittest="false"/>
						<Label id="HeroPowerValue" text="#HeroPowerValue" hittest="false" />
					</Panel>
					<!-- 玩家资源 -->
					<Panel id="PlayerResource" hittest="false">
					</Panel>
				</Panel>
			</Panel>
			<Panel id="top_bar" hittest="false">
			</Panel>

			<Panel id="upper_hud" hittest="false" disallowedstyleflags="hover,descendantfocus">
				<!-- <Panel id="TimeOfDayBG">
					<Label id="GameTime" text="{s:game_time_minutes}:{s:game_time_seconds}" />
				</Panel> -->
				<!-- 回合 -->
				<!-- <Panel id="Rounds" hittest="false">
					<Label id="RoundNumber" class="MonoNumbersFont" text="{d:round_number}" hittest="false" />
					<ProgressBar id="RoundProgressBar" hittest="false"/>
					<Label id="RoundTime" class="MonoNumbersFont" text="{d:round_time}" hittest="false" />
				</Panel> -->
			</Panel>
			<Panel id="MenuButtons"/>

			<Panel id="NpcContainer"/>
			<Panel id="PlayerContainer"/>
			<Panel id="HelpContainer" hittest="false"/>
			<Panel id="RoundStart" hittest="false"/>
			<Panel id="PostGame" hittest="false"/>
		</Panel>

		<Panel id="DroppedItemPanel" hittest="fasle" />
	</Panel>
</root>
